<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102" fx:controller="businessapplicationinformation.EmployeeViewController">
   <children>
      <TableView fx:id="EmployeeTable" layoutY="58.0" prefHeight="234.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="IDColumn" prefWidth="88.0" text="Employee ID" />
          <TableColumn fx:id="firstNameColumn" prefWidth="90.0" text="First Name" />
            <TableColumn fx:id="lastNameColumn" prefWidth="90.0" text="Last Name" />
            <TableColumn fx:id="phoneColumn" prefWidth="121.0" text="Phone Number" />
        </columns>
      </TableView>
      <Label layoutX="24.0" layoutY="14.0" text="List of Employees">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Button layoutX="24.0" layoutY="327.0" mnemonicParsing="false" onAction="#createButtonPushed" text="Create New Employee" />
      <Button fx:id="editButton" layoutX="187.0" layoutY="327.0" mnemonicParsing="false" onAction="#editButtonPushed" onMouseClicked="#ifemployeeselected" text="Edit Employee" />
   </children>
</AnchorPane>
